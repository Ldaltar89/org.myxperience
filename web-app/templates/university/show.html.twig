{% extends 'base.html.twig' %}

{% block title %}University{% endblock %}

{% block body %}
    <h1>University</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ university.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ university.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ university.type }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ university.location }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ university.website }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ university.email }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ university.phone }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ university.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ university.createdAt ? university.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedBy</th>
                <td>{{ university.createdBy }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ university.updatedAt ? university.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedBy</th>
                <td>{{ university.updatedBy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_university_index') }}">back to list</a>

    <a href="{{ path('app_university_edit', {'id': university.id}) }}">edit</a>

    {{ include('university/_delete_form.html.twig') }}
{% endblock %}
