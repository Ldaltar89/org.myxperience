{% extends 'base.html.twig' %}

{% block title %}Examns{% endblock %}

{% block body %}
    <h1>Examns</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ examn.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ examn.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ examn.description }}</td>
            </tr>
            <tr>
                <th>Score</th>
                <td>{{ examn.score }}</td>
            </tr>
            <tr>
                <th>Score_pass</th>
                <td>{{ examn.scorePass }}</td>
            </tr>
            <tr>
                <th>IsActive</th>
                <td>{{ examn.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ examn.createdAt ? examn.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedBy</th>
                <td>{{ examn.createdBy }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ examn.updatedAt ? examn.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedBy</th>
                <td>{{ examn.updatedBy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_examns_index') }}">back to list</a>

    <a href="{{ path('app_examns_edit', {'id': examn.id}) }}">edit</a>

    {{ include('examns/_delete_form.html.twig') }}
{% endblock %}
